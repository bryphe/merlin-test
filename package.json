{
  "name": "hello-reason",
  "version": "0.1.0",
  "description": "Reason workflow with Esy",
  "license": "MIT",
  "esy": {
    "build": "dune build -p #{self.name}",
    "NOTE": "Optional release Section. Customizes result of `esy release`",
    "release": {
      "releasedBinaries": [
        "Hello.exe"
      ],
      "deleteFromBinaryRelease": [
        "*esy_ocaml_*",
        "*refmterr-*",
        "ocaml-*"
      ]
    },
    "buildsInSource": "_build"
  },
  "dependencies": {
    "@opam/dune": "*",
    "@opam/lambda-term": "*",
    "@opam/lwt": "*",
    "@esy-ocaml/reason": "*",
    "ocaml": "~4.6.0",
    "@opam/merlin": "*"
  },
  "resolutions": {
    "@esy-ocaml/esy-installer": "0.0.1",
    "@esy-ocaml/reason": "link:../real-reason",
    "@opam/menhir": "20171013",
    "@opam/re": "1.8.0"
  },
  "devDependencies": {
    "ocaml": "~4.6.0"
  }
}
